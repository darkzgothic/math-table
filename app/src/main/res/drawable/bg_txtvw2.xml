<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF9252" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#FFBE6A" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="8dp"
                android:top="4dp"
                android:right="8dp"
                android:bottom="4dp" />
        </shape>
    </item>
</selector>